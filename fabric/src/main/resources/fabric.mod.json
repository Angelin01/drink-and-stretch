{
  "schemaVersion": 1,
  "id": "${modId}",
  "version": "${version}",
  "name": "${modName}",
  "description": "${description}",
  "authors": [
    "${authors}"
  ],
  "icon": "${iconLogo}",
  "contact": {
    "homepage": "${homepage}",
    "issues": "${issues}"
  },
  "license": "${license}",
  "environment": "client",
  "entrypoints": {
    "main": [
      "com.angelin01.drinkandstretch.fabric.DrinkAndStretchFabric"
    ],
    "modmenu": [
      "com.angelin01.drinkandstretch.fabric.ModMenuIntegration"
    ]
  },
  "mixins": [
    "drinkandstretch.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.18.4",
    "minecraft": "~1.21.1",
    "java": ">=21",
    "fabric-api": "*"
  }
}
